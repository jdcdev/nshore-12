<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<template id="report_purchasequotation_document_inherit" inherit_id="purchase.report_purchasequotation_document">
		<xpath expr="//table/thead/tr/th[3]" position="after">
            <th class="text-right"><strong>Received Qty</strong></th>
        </xpath>
        <xpath expr="//table/tbody/tr/td[3]" position="after">
            <td class="text-right">
                <span t-field="order_line.qty_received"/>
            </td>
		</xpath>
        <xpath expr="//tr[@t-foreach='o.order_line']/td[1]" position="replace">
            <t t-if="order_line.code_product">
                <td>
                    <span t-field="order_line.product_id.display_name"/>
                    <br/>
                    <span t-field="order_line.code_product"/>
                </td>
            </t>
            <t t-else = "">
               <td>
                   <span t-field="order_line.name"/>
                </td> 
            </t>
        </xpath>
	</template>
</odoo>