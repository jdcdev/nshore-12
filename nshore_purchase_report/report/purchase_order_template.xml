<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<template id="report_purchaseorder_document_inherit" inherit_id="purchase.report_purchaseorder_document">
		<xpath expr="//table[hasclass('o_main_table')]/thead/tr/th[4]" position="after">
			<th class="text-right"><strong>Received Qty</strong></th>  
		</xpath>
		<xpath expr="//table[hasclass('o_main_table')]/tbody/tr/td[4]" position="after">
			<td class="text-right">
                <span t-field="line.qty_received"/>
            </td>  
		</xpath>
	</template>
</odoo>