<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>

        <report
                id="report_return_order"
                model="return.order"
                string="Return Order"
                name="sale_return.report_return_order_view"
                file="sale_return.report_return_order_view"
                report_type="qweb-pdf"/>

        <template id="report_return_order_view">
            <t t-call="web.html_container">
                <t t-foreach="docs" t-as="doc">
                    <t t-call="web.external_layout">
                        <div class="page">
                            <div class="oe_structure"/>
                            <div class="row">
                                <div class="col-6">
                                    <span t-field="doc.partner_id"
                                        t-options='{"widget": "contact", "fields": ["address", "name"], "no_marker": True}' />
                                </div>
                                <div class="col-6">
                                    <h2>Return Order</h2>
                                    <div class="row">

                                    </div>
                                </div>
                            </div>
                        </div>
                    </t>
                </t>
            </t>
        </template>

    </data>
</odoo>